<%- include('../includes/head.ejs') %>
    <div class="container">
        <%- include('./dashboard-includes/sidebar.ejs') %>
            <main class="main">
                <%- include('./dashboard-includes/topbar.ejs') %>
                    <!-- WORKSPACE NAME HEADER -->
                    <div style="padding:20px;width:97%;display:flex; justify-content:space-between; align-items: center;border-bottom: 1px solid rgba(51, 51, 51, 0.539); margin: auto;"
                        class="dashboard-header">
                        <h2>Sidak's Workspace</h2>
                        <a class="btn2" href="/newSurvey"> Start New Survey <i class="bi bi-plus-square-fill"></i></a>
                    </div>
                    <!-- RECENT SURVEYS -->
                    <div style="padding:10px 20px; color:gray; padding-bottom: 0;display:flex; justify-content:space-between; align-items: center;width:
                    97%;margin: auto;">
                        <h2>Your Recent Surveys</h2>
                        <p class="text-muted"><a class="text-muted" href="/">View All...</a></p>
                    </div>
                    <!-- Survey grids -->
                    <div id="surveyList">
                        <%for(let i=1;i<=4;i++){%>
                            <!-- Survey Card Begins -->
                            <div class="survey-card-dashboard">
                                <div class="survey-image">
                                    <img src="data:image/jpeg;base64,"
                                        alt="">
                                </div>
                                <!-- Survey Data -->
                                <div class="survey-data">
                                    <h2>Best Color Codes For Website</h2>
                                    <div class="text-muted">
                                        <p>Responses: 5</p>
                                        <p>Last Updated: 2022-03-11</p>
                                        <p>Created On: 2022-01-11</p>
                                    </div>
                                    <!-- Dot menu -->
                                    <div class="dot-menu">
                                        <div style="padding: 10px; cursor: pointer; font-size: 18px;" class="dropdown">
                                            <span class="profile-navbar"><i
                                                    class="bi bi-three-dots-vertical"></i></span>
                                            <div class="dropdown-content">
                                                <div class="dropdown-container">
                                                    <ul class="dropdown-list">
                                                        <li><a href="/">Share</a></li>
                                                        <li><a href="/">Responses</a></li>
                                                        <li><a href="/mySurveys">View</a></li>
                                                        <li><a href="/">Edit</a></li>
                                                        <li>
                                                            <form action=""><button class="delete-button"
                                                                    type="submit">Delete</button>
                                                            </form>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <%}%>
                    </div>
                    <%- include('./dashboard-includes/footer.ejs') %>
            </main>
    </div>
    <%- include('../includes/end.ejs') %>